always assume{
}

always guarantee {
    // Initially and when the user is not pressing the up button, set the counter (count) to 0.5
    (! pressUp(e)) -> [count <- 0.5];
    // When the user presses the up button, increment the counter by 1 each frame
    (pressUp(e)) -> [count <- count + 1];
    // Update the cube's scale along the x-axis with the value of the counter
    [cube.scale.x <- count];
}
