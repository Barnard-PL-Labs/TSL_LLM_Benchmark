always assume {
  // Assumptions about button presses
  (!button1 || !button2) && (!button1 || !button3) && (!button2 || !button3);
}

always guarantee {
  // Guarantee 1: When button1 is pushed, E4 will eventually be played on noteToPlay
  button1 -> F ([noteToPlay <- "E4"]);

  // Guarantee 2: When button2 is pushed, G4 will eventually be played on noteToPlay
  button2 -> F ([noteToPlay <- "G4"]);

  // Guarantee 3: When button3 is pushed, A4 will eventually be played on noteToPlay2
  button3 -> F ([noteToPlay2 <- "A4"]);

  // Guarantee 4: Immediate action for button1 and button2
  button1 -> X ([noteToPlay <- "E4"]);
  button2 -> X ([noteToPlay <- "G4"]);

  // Guarantee 5: Exclusive playing of E4 on noteToPlay and A4 on noteToPlay2
  ([noteToPlay2 <- "A4"]) -> !(noteToPlay = "E4");
  ([noteToPlay <- "E4"]) -> !(noteToPlay2 = "A4");

  // Guarantee 6: A4 cannot be played on noteToPlay2 until E4 has stopped being played on noteToPlay
  G ([noteToPlay = "E4"] -> (! ([noteToPlay2 <- "A4"]) W [noteToPlay <- ""]));

  // Guarantee 7: Sequence or chord playing
  ([noteToPlay <- "E4"]) -> F ([noteToPlay2 <- "A4"] && [noteToPlay <- "G4"]);
}
