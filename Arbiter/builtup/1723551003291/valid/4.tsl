
        always assume {
          F(r 1);  F(r 2);  F(r 3); 
        }
        always guarantee {
            (r 1) -> F ([ g <- 1 ]);
            (r 2) -> F ([ g <- 2 ]);
            (r 3) -> F ([ g <- 3 ]);
            ((p 2 1) && [ g <- 3 ] ->  ! ([ g <- 2 ]) W [ g <- 1 ]);
            ((p 3 1) && [ g <- 2 ] ->  ! ([ g <- 3 ]) W [ g <- 1 ]);
            ([ g <- 1 ] -> ((! ([ g <- 3 ]) W [ g <- 2 ])));
            ([ g <- 3 ] -> ((! ([ g <- 1 ]) W [ g <- 2 ])));
        }
    