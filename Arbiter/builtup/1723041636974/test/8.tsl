
        always assume {
          F (r t);
        }
        always guarantee {
            (r 1) -> F ([ g <- 1 ]);
            (r 2) -> F ([ g <- 2 ]);
            (r 3) -> F ([ g <- 3 ]);
            (r 4) -> F ([ g <- 4 ]);
            (r 5) -> F ([ g <- 5 ]);
            ((p 3 4) && [ g <- 1 ] ->  ! ([ g <- 3 ]) W [ g <- 4 ]);
            ((p 4 5) && [ g <- 1 ] ->  ! ([ g <- 4 ]) W [ g <- 5 ]);
            ((p 5 1) && [ g <- 4 ] ->  ! ([ g <- 5 ]) W [ g <- 1 ]);
            ([ g <- 1 ] -> ! ([ g <- 2 ]) W [ g <- 3 ]);
            ([ g <- 1 ] -> ! ([ g <- 3 ]) W [ g <- 2 ]);
            ([ g <- 2 ] -> ! ([ g <- 4 ]) W [ g <- 3 ]);
            ([ g <- 2 ] -> ! ([ g <- 4 ]) W [ g <- 5 ]);
            ([ g <- 3 ] -> ! ([ g <- 4 ]) W [ g <- 5 ]);
        }
    