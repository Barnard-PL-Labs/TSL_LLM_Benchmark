
        always assume {
          F(r 1);  F(r 2);  F(r 3);  F(r 4); 
        }
        always guarantee {
            (r 1) -> F ([ g <- 1 ]);
            (r 2) -> F ([ g <- 2 ]);
            (r 3) -> F ([ g <- 3 ]);
            (r 4) -> F ([ g <- 4 ]);
            ((p 1 2) && [ g <- 3 ] ->  ! ([ g <- 1 ]) W [ g <- 2 ]);
            ((p 1 2) && [ g <- 4 ] ->  ! ([ g <- 1 ]) W [ g <- 2 ]);
            ((p 1 3) && [ g <- 2 ] ->  ! ([ g <- 1 ]) W [ g <- 3 ]);
            ((p 2 1) && [ g <- 3 ] ->  ! ([ g <- 2 ]) W [ g <- 1 ]);
            ((p 2 1) && [ g <- 4 ] ->  ! ([ g <- 2 ]) W [ g <- 1 ]);
            ((p 2 3) && [ g <- 1 ] ->  ! ([ g <- 2 ]) W [ g <- 3 ]);
            ((p 2 3) && [ g <- 4 ] ->  ! ([ g <- 2 ]) W [ g <- 3 ]);
            ((p 2 4) && [ g <- 1 ] ->  ! ([ g <- 2 ]) W [ g <- 4 ]);
            ((p 2 4) && [ g <- 3 ] ->  ! ([ g <- 2 ]) W [ g <- 4 ]);
            ((p 3 1) && [ g <- 2 ] ->  ! ([ g <- 3 ]) W [ g <- 1 ]);
            ((p 3 1) && [ g <- 4 ] ->  ! ([ g <- 3 ]) W [ g <- 1 ]);
            ((p 3 2) && [ g <- 1 ] ->  ! ([ g <- 3 ]) W [ g <- 2 ]);
            ((p 3 4) && [ g <- 1 ] ->  ! ([ g <- 3 ]) W [ g <- 4 ]);
            ((p 3 4) && [ g <- 2 ] ->  ! ([ g <- 3 ]) W [ g <- 4 ]);
            ((p 4 1) && [ g <- 2 ] ->  ! ([ g <- 4 ]) W [ g <- 1 ]);
            ((p 4 1) && [ g <- 3 ] ->  ! ([ g <- 4 ]) W [ g <- 1 ]);
            ((p 4 2) && [ g <- 1 ] ->  ! ([ g <- 4 ]) W [ g <- 2 ]);
            ((p 4 3) && [ g <- 1 ] ->  ! ([ g <- 4 ]) W [ g <- 3 ]);
            ((p 4 3) && [ g <- 2 ] ->  ! ([ g <- 4 ]) W [ g <- 3 ]);
            ([ g <- 1 ] -> ((! ([ g <- 3 ]) W [ g <- 2 ])));
            ([ g <- 1 ] -> ((! ([ g <- 3 ]) W [ g <- 4 ])));
            ([ g <- 1 ] -> ((! ([ g <- 4 ]) W [ g <- 2 ])));
            ([ g <- 2 ] -> ((! ([ g <- 1 ]) W [ g <- 3 ])));
            ([ g <- 2 ] -> ((! ([ g <- 1 ]) W [ g <- 4 ])));
            ([ g <- 2 ] -> ((! ([ g <- 3 ]) W [ g <- 4 ])));
            ([ g <- 3 ] -> ((! ([ g <- 2 ]) W [ g <- 1 ])));
            ([ g <- 3 ] -> ((! ([ g <- 4 ]) W [ g <- 1 ])));
            ([ g <- 3 ] -> ((! ([ g <- 4 ]) W [ g <- 2 ])));
            ([ g <- 4 ] -> ((! ([ g <- 1 ]) W [ g <- 3 ])));
            ([ g <- 4 ] -> ((! ([ g <- 2 ]) W [ g <- 1 ])));
        }
    