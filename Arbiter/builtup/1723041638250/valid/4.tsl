            (r 5) -> F ([ g <- 5 ]);
            ([ g <- 2 ] -> ! ([ g <- 1 ]) W [ g <- 5 ]);
            ([ g <- 2 ] -> ! ([ g <- 4 ]) W [ g <- 3 ]);
            ([ g <- 2 ] -> ! ([ g <- 4 ]) W [ g <- 5 ]);
            ([ g <- 3 ] -> ! ([ g <- 1 ]) W [ g <- 4 ]);
        }
    