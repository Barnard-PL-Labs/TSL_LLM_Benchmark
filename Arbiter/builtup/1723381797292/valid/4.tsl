
        always assume {
          F (r t);
        }
        always guarantee {
            (r 1) -> F ([ g <- 1 ]);
            (r 2) -> F ([ g <- 2 ]);
            (r 3) -> F ([ g <- 3 ]);
            ((p 1 3) && [ g <- 2 ] ->  ! ([ g <- 1 ]) W [ g <- 3 ]);
            ((p 2 1) && [ g <- 3 ] ->  ! ([ g <- 2 ]) W [ g <- 1 ]);
            ((p 2 3) && [ g <- 1 ] ->  ! ([ g <- 2 ]) W [ g <- 3 ]);
            ([ g <- 3 ] -> ((! ([ g <- 1 ]) W [ g <- 2 ])));
        }
    